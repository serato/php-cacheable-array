{
	"name": "serato/cacheable-array",
	"homepage": "https://github.com/serato/php-cachable-arrray",
	"description": "Provides an array-like object whose contents are persisted to a PSR-16 cache",
	"type": "library",
	"require": {
		"php" : "^7.1 || ^8.0",
		"psr/simple-cache": "^1.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7 || ^8",
		"squizlabs/php_codesniffer": "^3.0",
		"symfony/cache": "^3",
		"phpstan/phpstan": "^1.4"
	},
	"autoload": {
		"psr-4": {
			"Serato\\CacheableArray\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\CacheableArray\\Test\\": "tests/"
		}
	}
}
